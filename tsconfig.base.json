{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": false,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "target": "ESNext",
    "types": []
  },
  "exclude": ["**/node_modules", "**/dist"],
  "typeAcquisition": {
    "enable": false
  },
  "typeRoots": ["./node_modules/@types", "./typings"]
}
